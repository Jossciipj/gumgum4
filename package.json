{
  "name": "gumgum-common-js",
  "version": "0.1.0",
  "description": "GumGum JavaScript Components",
  "main": "dist/index",
  "module": "dist-es6/index",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "prebuild": "rimraf dist dist-es6",
    "build": "yarn build:es5 && yarn build:es6",
    "build:es5": "babel components --out-dir dist --plugins=transform-es2015-modules-commonjs --source-maps inline",
    "build:es6": "babel components --out-dir dist-es6 --source-maps inline",
    "buildstorybook": "build-storybook -c .storybook -o .out",
    "lint": "eslint ./components --color",
    "test": "storyshots",
    "postinstall": "postinstall-build --only-as-dependency dist",
    "preclean": "rimraf node_modules",
    "clean": "yarn cache clean && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/gumgum/common-js-components.git"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.3.0",
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-deployer": "^1.0.0",
    "@kadira/storyshots": "^2.1.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "eslint": "^3.18.0",
    "eslint-plugin-react": "^6.10.3",
    "jest-cli": "^19.0.2",
    "raw-loader": "^0.5.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^3.0.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.12.3"
  },
  "dependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "postinstall-build": "^5.0.0",
    "react-modal": "^1.7.3"
  }
}
